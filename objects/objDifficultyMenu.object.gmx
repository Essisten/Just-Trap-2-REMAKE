    draw_set_halign(fa_center);
    
    if (difSelect &amp;&amp; i == select)
    {
        if (!warnText)
        {
            if(select2==-1)
                scrDrawTextOutline(x + i * xSeperation + 65, y + 49, "&lt; Load game &gt;" ,clr , c_black);
            else
                scrDrawTextOutline(x + i*xSeperation + 65, y + 49, "&lt; " + dif[select2] + " &gt;" ,clr , c_black);
        }
        else
