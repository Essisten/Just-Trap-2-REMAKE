draw_set_alpha(1);
for(var i = 0; i &lt; optionsNum; i++)
{
    var clr = c_white;
    var value = "Off"
    if (i == select)
        clr = c_red;
    draw_set_halign(fa_left);
    scrDrawTextOutline(xSelector,ySelector+(ySeperation*i),strSelect[i], clr, c_black);
    draw_set_halign(fa_right);
    switch (i)
    {
        case 0:
            if (global.fullscreenMode)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector, value, clr, c_black);
            break;
        case 1:
            if (global.smoothingMode)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector+ySeperation, value, clr, c_black);
            break;
        case 2:
            if (global.showCard)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*2),value, clr, c_black);
            break;
        case 3:
            if (global.particles)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*3),value, clr, c_black);
            break;
        case 4:
            if (!global.delayBow)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*4),value, clr, c_black);
            break;
        case 5:
            if (global.showPB)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*5),value, clr, c_black);
            break;
        case 6:
            if (global.showNames)
                value = "On";
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*6),value, clr, c_black);
            break;
        case 7:
             style[0] = "New";
             style[1] = "Old";
             value = style[global.spriteStyle];
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*7),value, clr, c_black);
            break;
        case 8:
             GameOver[0] = "New";
             GameOver[1] = "New (dots)";
             GameOver[2] = "New (shiny)";
             GameOver[3] = "Old";
             value = GameOver[global.GameOverType];
            scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*8),value, clr, c_black);
             break;
    }
}
