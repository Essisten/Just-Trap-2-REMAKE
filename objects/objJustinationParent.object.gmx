{
    instance_destroy(bullet);
    exit;
}
var heal = bullet.doHeal;
instance_destroy(bullet);
if (HP &gt; 0 and !heal)
{
    HP -= 4 - global.difficulty;
    if (HP &gt; 0)
        scrPlaySound(sndBossHit);
    else
        scrPlaySound(sndDeath);
