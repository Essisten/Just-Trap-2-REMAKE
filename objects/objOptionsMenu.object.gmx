          <argument>
            <kind>1</kind>
            <string>draw_set_alpha(0.8);
draw_set_colour(c_black);
draw_rectangle(xSelector - offset, ySelector - offset, xSelector + xSeperation + offset, ySelector + ySeperation * optionsNum + offset, false);
draw_set_font(fDefault30);
draw_set_halign(fa_left);
draw_set_alpha(1);
for(var i = 0; i &lt; optionsNum; i++)
{
    var clr = c_white;
    if (i == select)
        clr = c_red;
    draw_set_halign(fa_left);
    scrDrawTextOutline(xSelector,ySelector+(ySeperation*i),strSelect[i], clr, c_black);
    draw_set_halign(fa_right);
    switch (i)
    {
        case 0:
            scrDrawTextOutline(xSelector+xSeperation,ySelector,string(global.musicVol) + "%", clr, c_black);
            break;
        case 1:
            scrDrawTextOutline(xSelector+xSeperation,ySelector+ySeperation,string(global.soundVol) + "%", clr, c_black);
            break;
        case 2:
            if (global.vsyncMode)
                scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*2),"On", clr, c_black);
            else
                scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*2),"Off", clr, c_black);
            break;
        case 3:
            if (global.soundTest)
                scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*3),"On", clr, c_black);
            else
                scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*3),"Off", clr, c_black);
            break;
        case 4:
            if (global.gravityControls)
                scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*4),"New", clr, c_black);
            else
                scrDrawTextOutline(xSelector+xSeperation,ySelector+(ySeperation*4),"Old", clr, c_black);
            break;
    }
}
</string>
