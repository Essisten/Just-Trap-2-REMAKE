        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (scrButtonCheckPressed(global.menuUpButton))
{
    scrPlaySound(sndBoss1_Reload, 1.5);
    select -= 1;
    if(select &lt; 0)
        select = optionsNum-1;
}
else if (scrButtonCheckPressed(global.menuDownButton))
{
    scrPlaySound(sndBoss1_Reload, 1.5);
    select += 1;
    if(select &gt; optionsNum-1)
        select = 0;
}
else if (scrButtonCheckPressed(global.menuBackButton))
{
    scrPlaySound(sndBoss1_Reload);
    //save changes and go back
    scrSaveConfig();
    global.menuSelectPrev[1] = 0;
    scrCreateFade(c_black, 0.05, true);
    alarm[1] = 20;
    exit;
}
else if (scrButtonCheckPressed(global.menuAcceptButton) or
        scrButtonCheckPressed(global.menuLeftButton) or
        scrButtonCheckPressed(global.menuRightButton) or
        scrButtonCheckPressed(global.menuOptionsButton))
{
    scrPlaySound(sndBoss1_Reload);
    global.menuSelectPrev[1] = select;
    switch (select)
    {
        case 0:
            if (scrButtonCheckPressed(global.menuLeftButton))
                global.musicVol--;
            else if (scrButtonCheckPressed(global.menuAcceptButton))
                global.musicVol += 10;
            else
                global.musicVol++;
                
            if (global.musicVol &gt; 100)
                global.musicVol -= 101;
            if (global.musicVol &lt; 0)
                global.musicVol += 101;
            audio_sound_gain(global.currentMusic, global.musicVol/100, 0);
            break;
        case 1:
            if (scrButtonCheckPressed(global.menuLeftButton))
                global.soundVol--;
            else if (scrButtonCheckPressed(global.menuAcceptButton))
                global.soundVol += 10;
            else
                global.soundVol++;
                
            if (global.soundVol &gt; 100)
                global.soundVol -= 101;
            if (global.soundVol &lt; 0)
                global.soundVol += 101;
            break;
        case 2:
            global.vsyncMode = !global.vsyncMode;
            scrSetVsync();
            break;
        case 3:
            global.soundTest = !global.soundTest;
            break;
        case 4:
            global.gravityControls = !global.gravityControls;
            break;
        case 5:
            scrSaveConfig();
            instance_create(x, y, objGraphicMenu);
            instance_destroy();
            break;
        case 6:
            scrSaveConfig();    //save changes
            global.menuSelectPrev[1] = select;
            instance_create(x,y,objKeyboardControlsMenu);
            instance_destroy();
            break;
        case 7:
            scrSaveConfig();    //save changes
            global.menuSelectPrev[1] = select;
            instance_create(x,y,objControllerMenu);
            instance_destroy();
            break;
    }
}
</string>
          </argument>
        </arguments>
      </action>
